      &lt;td&gt;
        &lt;p&gt;EGRA&lt;/p&gt;
      &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;
        &lt;p&gt;TIMSS &lt;/p&gt;
      &lt;/td&gt;
      &lt;td&gt;
        &lt;p&gt;EGMA&lt;/p&gt;
      &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;
        &lt;p&gt;PIRLS &lt;/p&gt;
      &lt;/td&gt;
      &lt;td&gt;
        &lt;p&gt;PAL network&amp;#xA0;&lt;/p&gt;
      &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td rowspan=&quot;7&quot;&gt;
        &lt;p&gt;&lt;strong&gt;End of primary&lt;/strong&gt;&lt;/p&gt;
      &lt;/td&gt;
      &lt;td&gt;
        &lt;p&gt;LLECE&lt;/p&gt;
      &lt;/td&gt;
      &lt;td rowspan=&quot;7&quot;&gt;
        &lt;p&gt;Yes&lt;/p&gt;
      &lt;/td&gt;
      &lt;td rowspan=&quot;7&quot;&gt;
        &lt;p&gt;PAL network&lt;/p&gt;
      &lt;/td&gt;
      &lt;td rowspan=&quot;7&quot;&gt;
        &lt;p&gt;plus or minus one year of last year of primary according to ISCED level of the country&lt;/p&gt;
      &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;
        &lt;p&gt;PASEC&lt;/p&gt;
      &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;
        &lt;p&gt;TIMSS &lt;/p&gt;
      &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;
        &lt;p&gt;PIRLS &lt;/p&gt;
      &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;
        &lt;p&gt;PILNA&lt;/p&gt;

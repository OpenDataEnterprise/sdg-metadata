    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;
        &lt;p&gt;SACMEQ&lt;/p&gt;
      &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td rowspan=&quot;3&quot;&gt;
        &lt;p&gt;&lt;strong&gt;End of lower secondary&lt;/strong&gt;&lt;/p&gt;
      &lt;/td&gt;
      &lt;td&gt;
        &lt;p&gt;PISA&lt;/p&gt;
      &lt;/td&gt;
      &lt;td rowspan=&quot;3&quot;&gt;
        &lt;p&gt;Yes&lt;/p&gt;
      &lt;/td&gt;
      &lt;td rowspan=&quot;3&quot;&gt;
        &lt;p&gt;&lt;a href=&quot;https://www.younglives.org.uk/&quot;&gt;Young Lives&lt;/a&gt;&lt;/p&gt;
      &lt;/td&gt;
      &lt;td rowspan=&quot;3&quot;&gt;
        &lt;p&gt;plus two or minus one of last year of lower secondary according to ISCED level of the country&lt;/p&gt;
      &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;
        &lt;p&gt;PISA-D&lt;/p&gt;

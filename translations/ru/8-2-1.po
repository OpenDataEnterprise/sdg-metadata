msgctxt "SDG_GOAL"
msgid ""
"<p>Goal: Promote sustained, inclusive and sustainable economic growth, full "
"and productive employment and decent work for all</p>"
msgstr ""

msgctxt "SDG_TARGET"
msgid ""
"<p>Target: Achieve higher levels of economic productivity through "
"diversification, technological upgrading and innovation, including through "
"a focus on high-value added and labour-intensive sectors</p>"
msgstr ""

msgctxt "SDG_INDICATOR"
msgid "<p>Indicator: 8.2.1: Annual growth rate of real GDP per employed person</p>"
msgstr ""

msgctxt "META_LAST_UPDATE"
msgid "<p>Last updated: March 2019</p>"
msgstr ""

msgctxt "SDG_RELATED_INDICATORS"
msgid "<h1>Related indicators</h1>"

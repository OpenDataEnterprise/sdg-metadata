msgctxt "IMPUTATION"
msgid ""
"<h2>Treatment of missing values:</h2>\n"
"<ul>\n"
"  <li>At country level</li>\n"
"</ul>\n"
"<p>Missing values are not imputed. </p>\n"
"<ul>\n"
"  <li>At regional and global levels</li>\n"
"</ul>\n"
"<p>Missing values are not imputed.</p>"
msgstr ""

msgctxt "REG_AGG"
